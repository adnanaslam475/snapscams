{
  "name": "snapscams-be-api",
  "version": "1.0.0",
  "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
  "main": "index.js",
  "engines": {
    "node": "17.3.0"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node index.js",
    "server": "nodemon index.js",
    "format": "prettier --single-quote --check .",
    "swagger-autogen": "node ./swagger.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/snapscams/snapscams-be-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/snapscams/snapscams-be-api/issues"
  },
  "homepage": "https://gitlab.com/snapscams/snapscams-be-api#readme",
  "dependencies": {
    "@sentry/node": "^7.11.1",
    "@sentry/tracing": "^7.11.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-basic-auth": "^1.2.1",
    "express-oauth2-jwt-bearer": "^1.1.0",
    "express-openid-connect": "^2.8.0",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.9.0",
    "mongoose": "^6.5.3",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.19",
    "requests": "^0.3.0",
    "swagger-autogen": "^2.21.5",
    "swagger-ui-express": "^4.5.0"
  }
}
